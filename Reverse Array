class ReverseanArray {
    public void reverseArray(int arr[]) {
       
        int n = arr.length;
        int[] temp = new int[n];
  
        // Copy elements from original array to temp in reverse order
        for (int i = 0; i < n; i++)
            temp[i] = arr[n - i - 1];
  
        // Copy elements back to original array
        for (int i = 0; i < n; i++)
            arr[i] = temp[i];
    }
    }
